{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"type": "object",
	"additionalProperties": false,
	"required": [
        "keycloak_user"
	],
	"properties": {
		"keycloak_user": {
			"type": "object",
			"additionalProperties": false,
			"required": [
				"email"
			],
			"properties": {
				"email": {
					"type": "string",
					"format": "email",
					"minLength": 1,
					"maxLength": 254
				}
			}
		}
	}
}